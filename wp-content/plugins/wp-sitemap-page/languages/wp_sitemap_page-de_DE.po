msgid "Pages"
msgstr "Seiten"

#: home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_0_12_next/wp-sitemap-page.php:330
msgid "Posts by category"
msgstr "Beiträge nach Kategorie"

#: home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_0_12_next/wp-sitemap-page.php:406
msgid "Archives"
msgstr "Archive"

#: home/tony/websites/www-dev/public/wp_plugins/wp-sitemap-page/plugin_1_0_12_next/wp-sitemap-page.php:424
